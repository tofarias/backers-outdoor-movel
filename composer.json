{
    "name": "tfarias/backers-outdoor-movel-ii",
    "authors": [
        {
            "name": "tofarias",
            "email": "tiago.farias.poa@gmail.com"
        }
    ],
    "require":
    {
        "vlucas/phpdotenv": "^3.4",
        "xtreamwayz/pimple-container-interop": "^1.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Backers\\":"src/"
        }
    },
    "require-dev": {
        "robmorgan/phinx": "^0.10.8",
        "fzaninotto/faker": "^1.4"
    },
    "scripts": {
        "db:refresh": [
            "vendor\\bin\\phinx.bat rollback -t 0 -e development --ansi",
            "vendor\\bin\\phinx.bat migrate -e development --ansi",
            "vendor\\bin\\phinx.bat seed:run -e development --ansi"
        ]
    }
}
