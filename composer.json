{
    "name": "tfarias/backers-outdoor-movel-ii",
    "authors": [
        {
            "name": "Tiago Farias",
            "email": "tiago.farias.poa@gmail.com"
        }
    ],
    "require":
    {
        "vlucas/phpdotenv": "^3.4",
        "xtreamwayz/pimple-container-interop": "^1.0",
        "aura/router": "^3.1",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1",
        "twig/twig": "^2.0",
        "illuminate/database": "^5.8",
        "jasny/auth": "^1.0",
        "phpmailer/phpmailer": "^6.0",
        "plasticbrain/php-flash-messages": "^1.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Backers\\":"src/"
        }
    },
    "require-dev": {
        "robmorgan/phinx": "^0.10.8",
        "fzaninotto/faker": "^1.4"
    },
    "scripts": {
        "db:refresh-windows": [
            "vendor\\bin\\phinx.bat rollback -t 0 -e development --ansi",
            "vendor\\bin\\phinx.bat migrate -e development --ansi",
            "vendor\\bin\\phinx.bat seed:run -e development --ansi"
        ],
        "db:refresh-linux": [
            "vendor/bin/phinx rollback -t 0 -e development --ansi",
            "vendor/bin/phinx migrate -e development --ansi",
            "vendor/bin/phinx seed:run -e development --ansi"
        ]
    }
}
